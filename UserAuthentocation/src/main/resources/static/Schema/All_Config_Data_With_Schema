for Fourth Error : 

SELECT email, COUNT(*) 
FROM users 
GROUP BY email 
HAVING COUNT(*) > 1;


-- Example: keep one, delete others
DELETE FROM users
WHERE id NOT IN (SELECT MIN(id) FROM users GROUP BY email);


SELECT user_id, COUNT(*) 
FROM verification_tokens 
GROUP BY user_id 
HAVING COUNT(*) > 1;


DELETE FROM verification_tokens
WHERE id NOT IN (
    SELECT MAX(id) FROM verification_tokens GROUP BY user_id
);

Ensure Unique Constraints in DB

ALTER TABLE users ADD CONSTRAINT uc_email UNIQUE(email);

ALTER TABLE verification_tokens 
ADD CONSTRAINT uc_user_token UNIQUE(user_id);

	
	
	
